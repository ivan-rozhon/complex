{
  "name": "complex",
  "version": "0.0.1",
  "description": "Experimental NoSQL PHP CMS",
  "scripts": {
    "clean": "rimraf dist",
    "delete": "rimraf dist node_modules",
    "prestart": "npm run clean",
    "start": "mkdir dist",
    "poststart": "concurrently \"SET NODE_ENV=development && parallel-webpack --watch\" \"npm run serve\"",
    "prebuild": "npm run clean",
    "build": "SET NODE_ENV=production && parallel-webpack",
    "serve": "lws --port=9000 --dir=dist/"
  },
