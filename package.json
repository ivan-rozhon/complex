  "description": "Experimental NoSQL PHP CMS",
  "scripts": {
    "clean": "rimraf dist && mkdir dist",
    "prestart": "npm run clean",
    "start": "webpack-dev-server --config config/webpack-web.dev.js --inline --progress",
    "prebuild": "npm run clean",
    "build": "webpack --config config/webpack-web.prod.js --progress --profile --bail",
